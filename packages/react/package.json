{
  "name": "@wp-headless/react",
  "private": false,
  "version": "0.4.3",
  "repository": "https://github.com/wp-headless/fetch",
  "description": "React bindings for wp-headless/fetch",
  "author": "Andrew McLagan <andrewmclagan@gmail.com>",
  "keywords": [
    "wordpress",
    "api",
    "client",
    "react"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "files": [
    "README.md",
    "lib/"
  ],
  "bugs": {
    "url": "https://github.com/wp-headless/fetch/issues"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "NODE_ENV=production BABEL_ENV=cjs babel --config-file ../../babel.config.js ./src --out-dir ./lib/cjs",
    "build:esm": "NODE_ENV=production BABEL_ENV=esm babel --config-file ../../babel.config.js ./src --out-dir ./lib/esm",
    "storybook": "start-storybook --port 9001"
  },
  "dependencies": {
    "react": "^16.0.0",
    "swr": "^0.1.13"
  },
  "devDependencies": {
    "@storybook/react": "^5.2.8",
    "@wp-headless/client": "^0.4.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "react-dom": "^16.0.0"
  },
  "license": "MIT"
}
