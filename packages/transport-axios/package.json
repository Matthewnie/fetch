{
  "name": "@wp-headless/transport-axios",
  "private": false,
  "version": "0.1.0",
  "repository": "https://github.com/wp-headless/fetch",
  "description": "wp-headless/fetch axios transport layer",
  "license": "MIT",
  "author": "Andrew McLagan <andrew.mclagan@gmail.com>",
  "keywords": [
    "wordpress",
    "fetch",
    "api",
    "client"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "files": [
    "README.md",
    "lib/"
  ],
  "bugs": {
    "url": "https://github.com/wp-headless/fetch/issues"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "NODE_ENV=production BABEL_ENV=cjs babel --config-file ../../babel.config.js ./src --out-dir ./lib/cjs",
    "build:esm": "NODE_ENV=production BABEL_ENV=esm babel --config-file ../../babel.config.js ./src --out-dir ./lib/esm"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "isomorphic-form-data": "^2.0.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.17.0"
  }
}
